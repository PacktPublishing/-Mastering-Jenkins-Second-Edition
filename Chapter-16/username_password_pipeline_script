pipeline {
    agent any
	environment {
    	ARTIFACTORY_CREDS = credentials('artifactory_server_credentials')
	}
	stages {
		stage('testing_credentials') {
			steps {
				sh '''
				echo $ARTIFACTORY_CREDS
				echo $ARTIFACTORY_CREDS_USR
				echo $ARTIFACTORY_CREDS_PSW
				'''
			}
		}
	}
}